sole.error('Error sending password reset email:', errorMessage);\n            alert(`Error sending password reset email: ${errorMessage}`);\n        });\n}\n\nfunction backBtnClick(event) {\n    event.preventDefault(); \n    renderLoginForm();\n}"},"lineCount":null}},"error":null,"hash":"f2d69d577917e045fbad6f1a214880cb","cacheData":{"env":{}}}